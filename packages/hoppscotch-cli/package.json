{
  "name": "hopp-cli",
  "version": "0.1.0",
  "description": "A CLI to run Hoppscotch test scripts in CI environments.",
  "main": "dist/index.js",
  "bin": {
    "hopp-cli": "bin/hopp-cli"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpx tsup",
    "dev": "pnpx tsc --build src --watch",
    "debugger": "node debugger.js 9999",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "test": "jest"
  },
  "keywords": [
    "cli",
    "hoppscotch",
    "hopp-cli"
  ],
  "author": "Hoppscotch (support@hoppscotch.io)",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@relmify/jest-fp-ts": "^1.1.1",
    "@types/axios": "^0.14.0",
    "@types/chalk": "^2.2.0",
    "@types/commander": "^2.12.2",
    "@types/inquirer": "^7.3.3",
    "@types/inquirer-fuzzy-path": "^2.3.1",
    "@types/jest": "^27.4.0",
    "@types/table": "^6.3.2",
    "@types/tcp-ping": "^0.1.3",
    "esm": "^3.2.25",
    "io-ts": "^2.2.16",
    "jest": "^27.5.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@hoppscotch/data": "workspace:^0.4.0",
    "@hoppscotch/js-sandbox": "workspace:^2.0.0",
    "axios": "^0.21.4",
    "chalk": "^4.1.1",
    "commander": "^8.0.0",
    "fp-ts": "^2.11.3",
    "inquirer": "^8.1.1",
    "inquirer-fuzzy-path": "^2.3.0",
    "lodash": "^4.17.21",
    "qs": "^6.10.3",
    "table": "^6.7.1",
    "tcp-ping": "^0.1.1",
    "tsup": "^5.11.13"
  }
}
